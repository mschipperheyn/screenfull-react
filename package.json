{
  "name": "screenfull-react",
  "description": "Full screen experiences for mobile",
  "author": "Marc Schipperheyn (m.schipperheyn@gmail.com)",
  "user": "mschipperheyn",
  "version": "0.0.2",
  "keywords": [
    "react",
    "reactjs",
    "screenfull-react"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config examples/webpack.config.babel.js --hot",
    "build": "npm run build:lib && npm run build:dist",
    "prebuild:lib": "rm -rf lib/*",
    "build:lib": "babel --out-dir lib src",
    "prebuild:umd": "rm -rf dist/*",
    "prebuild:dist": "rm -rf dist/*",
    "build:dist": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "build:watch": "npm run build:lib -- --watch",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "demo:build": "cross-env NODE_ENV=production webpack --config examples/webpack.config.babel.js",
    "lint": "eslint 'src/**/*.js'",
    "prepublish": "npm run build"
  },
  "main": "dist/screenfull-react.js",
  "module": "dist/screenfull-react.es.js",
  "jsnext:main": "dist/screenfull-react.es.js",
  "dependencies": {
    "screenfull": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-loader": "^0.4.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-visualizer": "^3.2.3",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mschipperheyn/screenfull-react.git"
  },
  "homepage": "https://github.com/mschipperheyn/screenfull-react/",
  "bugs": {
    "url": "https://github.com/mschipperheyn/screenfull-react/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "license": "MIT",
  "pre-push": [
    "test"
  ]
}

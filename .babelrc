{
  "presets": ["es2015-rollup", "react"],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "start": {
      "presets": ["react-hmre"]
    },
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap",
            "ignoreFilenames": ["node_modules"]
          }
        ]
      ]
    }
  }
}
